function getIndexToIns(arr, num) {
  var newarr = [];

  arr.push(num);
  arr.sort((a,b) => a-b);
 
   
   for(var i=0; i<arr.length; i++){
     if(arr[i] === num){
       return 0;
     }else{

     return arr.indexOf(num)
     }
     }

}

let result =getIndexToIns([5, 3, 20, 3], 5);
console.log(result);
